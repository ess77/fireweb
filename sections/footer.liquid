  /* Bloc texte */
  @media screen and (min-width: 750px) {
    .footer-block__details-content > p {
        margin-right: 1.5rem;
        font-size: 1.4rem;
    }
  }

  {%- for block in section.blocks -%}
    /* Image SVG */
    .footer-block-image svg {
      width: {{ block.settings.image_width }}px;
    }

    /* Image mobile */
    @media screen and (max-width: 990px) {
      .footer-block-image > img, .logo-footer svg {
        width: calc({{ block.settings.image_width }}px - 20px);
      }
    }

    /* Menu */
    @media screen and (min-width: 749px) {
      .link_list--accordion {
        display: none;
      }
    }
    @media screen and (max-width: 749px) {
      .link_list_accordion, .title_menu {
        display: none;
      }
    }
  {%- endfor -%}

  .band_footer {
     bottom: 0;
     margin:0;
     color: white;
     padding-block: {{ section.settings.footer_band_height_desktop }};
     /*position: fixed;*/
     text-align: center;
     width: 100%;
     z-index:1;
     letter-spacing:1px;
     font-weight:{{ section.settings.footer_band_text_style }};
  }

  .band_footer{
     background-color:{{ section.settings.footer_band_bg_color }};
     color:{{ section.settings.footer_band_text_color }};
     font-size:{{ section.settings.footer_band_text_size }};
  }
  .mob_only, .band_footer a {
     text-decoration: none;
     color:{{ section.settings.footer_band_text_color }};
  }
  .band_footer a:hover{
     color:{{ section.settings.footer_band_link_hover_color }}!important;
  } 
 
  @media screen and (min-width: 240px) and (max-width: 481px) {
  .desk_only {
    display: none !important;
  }
  {% if section.settings.m_footer_band_text != blank %}
  .band_footer {
    display: none;
  }
  {% endif %}
  .mob_only{
    display: block !important;
  }
  }  

  .band_footer{
     border-top: {{ section.settings.footer_band_top_border_width }} solid {{ section.settings.footer_band_top_border_color }};
  }

  
{%- endstyle -%}

  
