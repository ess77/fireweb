{%- style -%}
  {% render 'section-style' %}

  {% render 'animation-hover-footer' %}

  .footer__content-bottom {
    background-color: {{ section.settings.background_footer-second }};
  }

  .footer {
    margin-top: {{ section.settings.margin_top | times: 0.75 | round: 0 }}px;
  }

  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .footer {
      margin-top: {{ section.settings.margin_top }}px;
    }

    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  /* Style de bordure supérieur */
  #wave {
    background-color: {{ settings.colors_background_1 }};
    fill: {{ section.settings.color_scheme }};
    padding: 0;
    margin-bottom: -1px;
  }
  #shopify-section-{{ section.id }} .footerwave_svg {
    width: 100%;
    overflow: hidden;
    text-align: center;
    z-index: 1;
    left: 0;
  }
  #shopify-section-{{ section.id }} .footerwave_svg #wave {
      display: block;
    }
  @media screen and (min-width: 768px) {
  #shopify-section-{{ section.id }} .footerwave_svg #wave {
      width: calc(100% + 0%);
      left: calc(-0%/2);
      position: relative;
    }
  }
  @media screen and (max-width: 768px) {
    #shopify-section-{{ section.id }} .footerwave_svg #wave {
      width: calc(100% + 0%);
      left: calc(-0%/2);
      position: relative;
    }
  }

  /*--- CSS Bordure personnalisé Fireweb ---*/
  #shopify-section-{{ section.id }} .footerborder_svg {
    top: 0;
    left: 0;
    width: 100%;
    overflow: hidden;
    text-align: center;
    z-index: 1;
  }
  #shopify-section-{{ section.id }} .footerborder_svg svg {
      display: block;
    }
  @media screen and (min-width: 768px) {
  #shopify-section-{{ section.id }} .footerborder_svg svg {
      position: relative;
      width: calc(100% + 1.3px);
      height: 100px;
      fill: #fff;
      margin-top: -1px;
    }
  }
  @media screen and (max-width: 768px) {
    #shopify-section-{{ section.id }} .footerborder_svg svg {
      position: relative;
      width: calc(100% + 1.3px);
      height: 50px;
      fill: #fff;
      margin-top: -1px;
    }
  }
  
  .section-{{ section.id }}.wave_custom1 {
      background-color: {{ section.settings.color_border_fond_svg }};
      fill: {{ section.settings.color_border_svg }};
