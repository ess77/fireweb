{{ 'section-custom-content.css' | asset_url | stylesheet_tag }}
{{ 'component-accordion.css' | asset_url | stylesheet_tag }}
{{ 'video-section.css' | asset_url | stylesheet_tag }}
{{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

<link rel="stylesheet" href="{{ 'component-slider.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-rte.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'section-main-page.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'section-before-slider.css' | asset_url }}" media="print" onload="this.media='all'">

<noscript>{{ 'component-slider.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-rte.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'section-main-page.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'section-before-slider.css' | asset_url | stylesheet_tag }}</noscript>

{%- style -%}
  {% render 'section-style' %}

  #shopify-section-{{ section.id }} .custom-content {
	  display: flex;
      align-items: {{ section.settings.alignment_align }};
	  min-height: {{ section.settings.min_height }}vh;
  }

  .custom-section-page-width {
    width: {{ settings.page_width }}px;
  }

  {% if section.settings.image_bg_desk != blank %}
    .custom-content-{{ section.id }} {
      background-image: url("{{ section.settings.image_bg_desk.src | img_url:'2000x' }}");
      background-size: cover;
      background-position: center;
      background-attachment: initial;
    }
  {% endif %}

  {% if section.settings.image_bg_mob != blank %}
    @media screen and (max-width: 749px) {
      .custom-content-{{ section.id }} {
        background-image: url("{{ section.settings.image_bg_mob.src | img_url:'1200x' }}");
        background-size: cover;
        background-position: center;
        background-attachment: initial;
      }
    }
  {% endif %}

  .section-{{ section.id }}-padding {
    {% if section.settings.full_width != false %}
      padding-top: calc({{ section.settings.padding_top }}px * 0.75);
      padding-bottom: calc({{ section.settings.padding_bottom }}px  * 0.75);
    {% else %}
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    {% endif %}
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

{%- liquid
  assign show_mobile_slider = false
-%}

<div class="custom-content custom-content-{{ section.id }} color-{{ section.settings.color_scheme }} gradient background-primary">
  <div class="{% if section.settings.full_width != true %}page-width{% endif %} section-{{ section.id }}-padding isolate custom-section-page-width">
    <slider-component class="slider-mobile-gutter">
      <ul class="custom-content-list contains-content-container justify-{{ section.settings.alignment_justify }} align-{{ section.settings.alignment_align }}" id="Slider-{{ section.id }}" role="list">
        {%- liquid
          assign highest_ratio = 0
          for block in section.blocks
            if block.settings.image.aspect_ratio > highest_ratio
              assign highest_ratio = block.settings.image.aspect_ratio
            endif
          endfor
        -%}
        {%- for block in section.blocks -%}
          {%- case block.type -%}
        	{%- when 'heading' -%}
              <li id="Slide-{{ section.id }}-{{ forloop.index }}" class="custom-content-list__item cstmtext-{{ block.settings.alignment }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}" {{ block.shopify_attributes }}>
                <div class="title-custom title-wrapper--self-padded-mobile title-wrapper--no-top-margin">
                  <{{ block.settings.heading_tag }} {% if settings.effect_apply_title_enable%} data-aos="{{ settings.animation_title_name }}"{% endif %} class="title {{ block.settings.heading_size }}">
                    {{ block.settings.title | escape }}
                      <div class="fe_h2_svg"><span style="fill:{{ block.settings.colorfeh2 }}; width:{{ block.settings.widthfeh2 }}rem;" {% if settings.effect_apply_enable%} data-aos="{{ settings.animation_name }}" data-aos-delay="600"{% endif %}> {%if block.settings.custom_svg_fe != blank %} {{ block.settings.custom_svg_fe }}{% endif %}</span>
                        </div>
                  </{{ block.settings.heading_tag }}>
                </div>
              </li>
        	{%- when 'content' -%}
              <li {% if settings.effect_apply_content_enable%} data-aos="{{ settings.animation_content_name }}" data-aos-delay="800"{% endif %} id="Slide-{{ section.id }}-{{ forloop.index }}" class="custom-content-list__item cstmtext-{{ block.settings.alignment }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}" {{ block.shopify_attributes }}>
                <div class="title-wrapper--self-padded-mobile">
                  {{ block.settings.content }}
                </div>
              </li>
            {%- when 'separator' -%}
              <li
              id="Slide-{{ section.id }}-{{ forloop.index }}"
              class="custom-content-list__item cstmtext-{{ block.settings.alignment }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}"
              style=" 
                border-bottom-style: solid;
                border-bottom-width: {{ block.settings.bbwidth }}px;
                width: {{ block.settings.bblength }}%;
                height: 100%;
                margin-top: {{ block.settings.mt }}px;
                margin-bottom: {{ block.settings.mb }}px;
                border-bottom-color: {{ block.settings.bordercolor }};"
              {{ block.shopify_attributes }}>
              </li>
        	{%- when 'column' -%}
              {%- assign empty_column = '' -%}
              {%- if block.settings.image == blank and block.settings.title == blank and block.settings.text == blank and block.settings.link_label == blank -%}
                {%- assign empty_column = 'custom-content-list__item--empty' -%}
              {%- endif -%}
              {% if block.settings.image != blank and block.settings.bgimg %}
                {%- assign empty_column = 'custom-content-list__item--empty' -%}
                {% style %}
                  .custom-content-list-{{ section.id }}.align-center { align-items: stretch; }
                  .custom-content-list-{{ section.id }} { background-size: cover; }
                  .custom-content-card-{{ section.id }} { display: flex; align-items: center; }
                  .centercustom-content-list__item--empty { text-align: center; }
                {% endstyle %}  
              {% endif %}
              <li
                {% if block.settings.image != blank and block.settings.bgimg %}
                style="background-image: url('{{ block.settings.image | image_url: width: 1200 }}');"
                {% endif %}
                id="Slide-{{ section.id }}-{{ forloop.index }}"
                class="custom-content-list__item custom-content-list-{{ section.id }} color-{{ block.settings.color_scheme }} {% if block.settings.column_alignment == 'center' %} center{% endif %}{{ empty_column }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}"
                {{ block.shopify_attributes }}>
                <div class="custom-content-card custom-content-card-{{ section.id }} content-container{% if block.settings.bgnone %} background-none-csm{% endif %}">
                  {%- if block.settings.image != blank and block.settings.bgimg != true -%}
                    {% if block.settings.image_ratio == 'adapt' or block.settings.image_ratio == 'circle' %}
                      {% assign spaced_image = true %}
                    {% endif %}
                    <div class="custom-content-card__image-wrapper custom-content-card__image-wrapper--{{ block.settings.image_width }}-width{% if block.settings.image_width != 'full' or spaced_image %} custom-content-card-spacing{% endif %}">
                      <div class="media media--transparent media--{{ block.settings.image_ratio }}"
                        {% if block.settings.image_ratio == 'adapt' %}
                          style="padding-bottom: {{ 1 | divided_by: highest_ratio | times: 100 }}%;"
                        {% endif %}>
                        <img
                          class="custom-content-card__image"
                          srcset="{%- if block.settings.image.width >= 275 -%}{{ block.settings.image | image_url: width: 275 }} 275w,{%- endif -%}
                            {%- if block.settings.image.width >= 550 -%}{{ block.settings.image | image_url: width: 550 }} 550w,{%- endif -%}
                            {%- if block.settings.image.width >= 710 -%}{{ block.settings.image | image_url: width: 710 }} 710w,{%- endif -%}
                            {%- if block.settings.image.width >= 1420 -%}{{ block.settings.image | image_url: width: 1420 }} 1420w,{%- endif -%}
                            {{ block.settings.image | image_url }} {{ block.settings.image.width }}w"
                          src="{{ block.settings.image | image_url: width: 550 }}"
                          sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                            (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %},
                            calc(100vw - 30px)"
                          alt="{{ block.settings.image.alt }}"
                          height="{{ block.settings.image.height }}"
                          width="{{ block.settings.image.width }}"
                          loading="lazy"
                        >
                      </div>
                    </div>
                  {%- endif -%}
                  <div class="custom-content-card__info">
                    {%- if block.settings.title != blank -%}
                      <{{ block.settings.heading_tag }} class="title {{ block.settings.heading_size }}">{{ block.settings.title | escape }}</{{ block.settings.heading_tag }}>
                    {%- endif -%}
                    {%- if block.settings.text != blank -%}
                      <div class="rte">{{ block.settings.text }}</div>
                    {%- endif -%}
                    {%- if block.settings.link_label != blank -%}
                      <a class="{% if block.settings.btn-style == "link" %}link animate-arrow{% else %}button button--primary{% endif %}" {% if block.settings.link == blank %}role="link" aria-disabled="true"{% else %}href="{{ block.settings.link }}"{% endif %}>{{ block.settings.link_label | escape }}{% if block.settings.btn-style == "link" %}<span class="icon-wrap">&nbsp;{% render 'icon-arrow' %}</span>{% endif %}</a>
                    {%- endif -%}
                  </div>
                </div>
              </li>
        	{%- when 'column_accordion' -%}
              {%- assign empty_column = '' -%}
                {%- if block.settings.image == blank and block.settings.title == blank and block.settings.text == blank and block.settings.link_label == blank -%}
                  {%- assign empty_column = 'custom-content-list__item--empty' -%}
                {%- endif -%}
                <li id="Slide-{{ section.id }}-{{ forloop.index }}" class="custom-content-list__item {% if block.settings.column_alignment == 'center' %} center{% endif %}{{ empty_column }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}" {{ block.shopify_attributes }}>
                  <div class="custom-content-card content-container{% if block.settings.bgnone %} background-none-csm{% endif %}">
                    <div class="custom-content-card__info">
                      {%- if block.settings.title != blank -%}
                        <{{ block.settings.heading_tag }} class="title {{ block.settings.heading_size }}">{{ block.settings.title | escape }}</{{ block.settings.heading_tag }}>
                      {%- endif -%}
                      {%- if block.settings.text != blank -%}
                        <div class="rte">{{ block.settings.text }}</div>
                      {%- endif -%}

                      {%- if block.settings.collapse != blank -%}
                      <div class="product__accordion accordion quick-add-hidden" {{ block.shopify_attributes }}>
                        <details id="Details-{{ block.id }}-{{ section.id }}">
                          <summary>
                            <div class="summary__title">
                              {% render 'icon-accordion', icon: block.settings.collapse_icon %}
                              <p class="accordion__title">
                                {{ block.settings.collapse }}
                              </p>
                            </div>
                            {% render 'icon-caret' %}
                          </summary>
                          <div class="accordion__content rte" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                            {{ block.settings.collapse_content }}
                          </div>
                        </details>
                      </div>
                      {%- endif -%}
                      {%- if block.settings.collapse1 != blank -%}
                      <div class="product__accordion accordion quick-add-hidden" {{ block.shopify_attributes }}>
                        <details id="Details-{{ block.id }}-{{ section.id }}">
                          <summary>
                            <div class="summary__title">
                              {% render 'icon-accordion', icon: block.settings.collapse_icon1 %}
                              <p class="accordion__title">
                                {{ block.settings.collapse1 }}
                              </p>
                            </div>
                            {% render 'icon-caret' %}
                          </summary>
                          <div class="accordion__content rte" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                            {{ block.settings.collapse_content1 }}
                          </div>
                        </details>
                      </div>
                      {%- endif -%}
                      {%- if block.settings.collapse2 != blank -%}
                      <div class="product__accordion accordion quick-add-hidden" {{ block.shopify_attributes }}>
                        <details id="Details-{{ block.id }}-{{ section.id }}">
                          <summary>
                            <div class="summary__title">
                              {% render 'icon-accordion', icon: block.settings.collapse_icon2 %}
                              <p class="accordion__title">
                                {{ block.settings.collapse2 }}
                              </p>
                            </div>
                            {% render 'icon-caret' %}
                          </summary>
                          <div class="accordion__content rte" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                            {{ block.settings.collapse_content2 }}
                          </div>
                        </details>
                      </div>
                      {%- endif -%}
                      {%- if block.settings.collapse3 != blank -%}
                      <div class="product__accordion accordion quick-add-hidden" {{ block.shopify_attributes }}>
                        <details id="Details-{{ block.id }}-{{ section.id }}">
                          <summary>
                            <div class="summary__title">
                              {% render 'icon-accordion', icon: block.settings.collapse_icon3 %}
                              <p class="accordion__title">
                                {{ block.settings.collapse3 }}
                              </p>
                            </div>
                            {% render 'icon-caret' %}
                          </summary>
                          <div class="accordion__content rte" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                            {{ block.settings.collapse_content3 }}
                          </div>
                        </details>
                      </div>
                      {%- endif -%}
                      {%- if block.settings.collapse4 != blank -%}
                      <div class="product__accordion accordion quick-add-hidden" {{ block.shopify_attributes }}>
                        <details id="Details-{{ block.id }}-{{ section.id }}">
                          <summary>
                            <div class="summary__title">
                              {% render 'icon-accordion', icon: block.settings.collapse_icon4 %}
                              <p class="accordion__title">
                                {{ block.settings.collapse4 }}
                              </p>
                            </div>
                            {% render 'icon-caret' %}
                          </summary>
                          <div class="accordion__content rte" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                            {{ block.settings.collapse_content4 }}
                          </div>
                        </details>
                      </div>
                      {%- endif -%}

                      {%- if block.settings.link_label != blank -%}
                        <a class="link animate-arrow" {% if block.settings.link == blank %}role="link" aria-disabled="true"{% else %}href="{{ block.settings.link }}"{% endif %}>{{ block.settings.link_label | escape }}<span class="icon-wrap">&nbsp;{% render 'icon-arrow' %}</span></a>
                      {%- endif -%}
                    </div>
                  </div>
                </li>
            {%- when 'image' -%}
                <li id="Slide-{{ section.id }}-{{ forloop.index }}" class="custom-content-list__item img_custom_section cstmtext-{{ block.settings.alignment }} {{ empty_column }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}" {{ block.shopify_attributes }}>
                    <img src="{{ block.settings.image.src | img_url:'720x' }}" alt="{{ block.settings.image.alt }}">
                </li>
        	{%- when 'video' -%}
              <li id="Slide-{{ section.id }}-{{ forloop.index }}" class="custom-content-list__item cstmtext-{{ block.settings.alignment }} {{ empty_column }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}" {{ block.shopify_attributes }}>
                <noscript>
                  <div class="video-section__media"
                    {% if block.settings.cover_image != blank %} style="padding-bottom: {{ 1 | divided_by: block.settings.cover_image.aspect_ratio | times: 100 }}%;"{% endif %}
                  >
                    <a href="{{ block.settings.video_url }}" class="video-section__poster media media--transparent media--landscape{% if block.settings.cover_image == blank %} video-section__placeholder{% endif %}">
                      {%- if block.settings.cover_image != blank -%}
                        <img
                          srcset="{%- if block.settings.cover_image.width >= 375 -%}{{ block.settings.cover_image | image_url: width: 375 }} 375w,{%- endif -%}
                            {%- if block.settings.cover_image.width >= 750 -%}{{ block.settings.cover_image | image_url: width: 750 }} 750w,{%- endif -%}
                            {%- if block.settings.cover_image.width >= 1100 -%}{{ block.settings.cover_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                            {%- if block.settings.cover_image.width >= 1500 -%}{{ block.settings.cover_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                            {%- if block.settings.cover_image.width >= 1780 -%}{{ block.settings.cover_image | image_url: width: 1780 }} 1780w,{%- endif -%}
                            {%- if block.settings.cover_image.width >= 2000 -%}{{ block.settings.cover_image | image_url: width: 2000 }} 2000w,{%- endif -%}
                            {%- if block.settings.cover_image.width >= 3000 -%}{{ block.settings.cover_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                            {%- if block.settings.cover_image.width >= 3840 -%}{{ block.settings.cover_image | image_url: width: 3840 }} 3840w,{%- endif -%}
                            {{ block.settings.cover_image | image_url }} {{ block.settings.cover_image.width }}w"
                          src="{{ block.settings.cover_image | image_url: width: 1920 }}"
                          sizes="{% if block.settings.full_width %}100vw{% else %}(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 }}px, (min-width: 750px) calc(100vw - 10rem), 100vw{% endif %}"
                          alt="{{ 'sections.video.load_video' | t: description: block.settings.description | escape }}"
                          loading="lazy"
                          width="{{ block.settings.cover_image.width }}"
                          height="{{ block.settings.cover_image.height }}"
                        >
                      {%- else -%}
                        {{ 'collection-2' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                      {%- endif -%}
                    </a>
                  </div>
                </noscript>
                <deferred-media class="video-section__media deferred-media no-js-hidden gradient global-media-settings{% if block.settings.full_width %} global-media-settings--full-width{% endif %}" data-media-id="{{ block.settings.video_url.id }}"
                  {% if block.settings.cover_image != blank %} style="padding-bottom: {{ 1 | divided_by: block.settings.cover_image.aspect_ratio | times: 100 }}%;"{% endif %}
                >
                  <button
                    id="Deferred-Poster-Modal-{{ block.settings.video_url.id }}"
                    class="video-section__poster media deferred-media__poster media--landscape"
                    type="button"
                    aria-label="{{ 'sections.video.load_video' | t: description: block.settings.description | escape }}"
                  >
                  {%- if block.settings.cover_image != blank -%}
                    <img
                      srcset="{%- if block.settings.cover_image.width >= 375 -%}{{ block.settings.cover_image | image_url: width: 375 }} 375w,{%- endif -%}
                        {%- if block.settings.cover_image.width >= 750 -%}{{ block.settings.cover_image | image_url: width: 750 }} 750w,{%- endif -%}
                        {%- if block.settings.cover_image.width >= 1100 -%}{{ block.settings.cover_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                        {%- if block.settings.cover_image.width >= 1500 -%}{{ block.settings.cover_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                        {%- if block.settings.cover_image.width >= 1780 -%}{{ block.settings.cover_image | image_url: width: 1780 }} 1780w,{%- endif -%}
                        {%- if block.settings.cover_image.width >= 2000 -%}{{ block.settings.cover_image | image_url: width: 2000 }} 2000w,{%- endif -%}
                        {%- if block.settings.cover_image.width >= 3000 -%}{{ block.settings.cover_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                        {%- if block.settings.cover_image.width >= 3840 -%}{{ block.settings.cover_image | image_url: width: 3840 }} 3840w,{%- endif -%}
                        {{ block.settings.cover_image | image_url }} {{ block.settings.cover_image.width }}w"
                      src="{{ block.settings.cover_image | image_url: width: 1920 }}"
                      sizes="{% if block.settings.full_width %}100vw{% else %}(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 }}px, (min-width: 750px) calc(100vw - 10rem), 100vw{% endif %}"
                      alt="{{ 'sections.video.load_video' | t: description: block.settings.description | escape }}"
                      loading="lazy"
                      width="{{ block.settings.cover_image.width }}"
                      height="{{ block.settings.cover_image.height }}"
                    >
                  {%- else -%}
                    {{ 'collection-2' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                  {%- endif -%}
                  <span class="deferred-media__poster-button motion-reduce">
                    {%- render 'icon-play' -%}
                  </span>
                  </button>
                  <template>
                    {%- if block.settings.video_url.type == 'youtube' -%}
                      <iframe src="https://www.youtube.com/embed/{{ block.settings.video_url.id }}?enablejsapi=1" class="js-youtube" allow="autoplay; encrypted-media" allowfullscreen title="{{ block.settings.description | escape }}"></iframe>
                    {%- else -%}
                      <iframe src="https://player.vimeo.com/video/{{ block.settings.video_url.id }}" class="js-vimeo" allow="autoplay; encrypted-media" allowfullscreen title="{{ block.settings.description | escape }}"></iframe>
                    {%- endif -%}
                  </template>
                </deferred-media>
              </li>
          {%- when 'before' -%}
            <div class="container-before-after {{ block.settings.taille }} {{ block.settings.taillemobile }}">
              <div id="before-after-slider" style="border-radius: {{ block.settings.before_after_radius }}px">
                <div id="before-image">
                  {% if block.settings.before_image != blank %}
                      <img id="img_before_slide" src= "{{ block.settings.before_image | image_url }}" loading="lazy" alt="before">
                  {% else %}
                      {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                  {% endif %}

                  {%- if block.settings.before_text != blank -%}
                    <p class="before-after__label before-after__label--left">
                      {{- block.settings.before_text | escape -}}
                    </p>
                  {%- endif -%}
                </div>
            
                <div id="after-image">
                  {% if block.settings.after_image != blank %}
                      <img src= "{{ block.settings.after_image | image_url }}" loading="lazy" alt="after">
                  {% else %}
                      {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                  {% endif %}

                  {%- if block.settings.after_text != blank -%}
                    <p class="before-after__label before-after__label--right">
                      {{- block.settings.after_text | escape -}}
                    </p>
                  {%- endif -%}
                  
                </div>
                <div id="resizer"></div>
              </div>
            </div>
          {%- when 'button' -%}
            <div class="btn-block-custom cstmtext-{{ block.settings.alignment }} {{ block.settings.taille }} {{ block.settings.taillemobile }}{% if block.settings.offdesk-bloc %} offdeskblock{% endif %}{% if block.settings.offmob-bloc %} offmobblock{% endif %}" {{ block.shopify_attributes }}>
              {%- if block.settings.button_label != blank -%}
                <a class="button color-{{ block.settings.color_scheme }}{% if block.settings.button_style_secondary %} button--secondary{% else %} button--primary{% endif %}"{% if block.settings.button_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ block.settings.button_link }}"{% endif %}>
                  {{ block.settings.button_label | escape }}
                </a>
              {%- endif -%}
            </div>
          {%- when 'accordion' -%}
